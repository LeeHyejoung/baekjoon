#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>

int main(void)
{
	int n;
	int i, j;

	scanf("%d", &n);

	for (i = 0; i < n * 2; i++) {
		if (i % 2 == 0) {
			for (j = 0; j < n; j++) {
				if (j % 2 == 0) {
					printf("*");
				}
				else {
					printf(" ");
				}
			}
		}
		else {
			for (j = 0; j < n; j++) {
				if (j % 2 == 0) {
					printf(" ");
				}
				else {
					printf("*");
				}
			}
		}
		printf("\n");
	}

	return 0;
}
