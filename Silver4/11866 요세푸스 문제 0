import queue

n, k = map(int, input().split())
q = queue.Queue()

for i in range(1, n + 1):
    q.put(i)

answer = []

count = 1
for i in range(1, n + 1):
    while count < k:
        num = q.get()
        q.put(num)
        count += 1
    if count == k:
        answer.append(q.get())
        count = 1

print("<", end = "")
for i in range(n):
    print(answer[i], end = "")
    if i < n - 1:
        print(", ", end = "")
print(">")
